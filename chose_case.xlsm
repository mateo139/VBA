Public Function ODCZYT_TYPU(typ As String) As Variant

Dim wynik As Integer

Select Case typ
    Case Is = "a"
        wynik = 1
    Case Is = "b"
        wynik = 1
    Case Is = "c"
        wynik = 2
    Case Is = "dy"
        wynik = 3
    Case Is = "e"
        wynik = 4
    Case Is = "f"
        wynik = 4
    Case Is = "g"
        wynik = 5
        
End Select

ODCZYT_TYPU = wynik

End Function
