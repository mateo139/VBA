Sub delete_print_areas()

    'ActiveWorkbook.Save
    
    Sheets(1).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(2).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(3).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(4).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(5).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(6).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(7).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(8).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(9).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(10).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(11).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(12).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(13).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(14).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(15).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    Sheets(16).Activate
    ActiveSheet.PageSetup.PrintArea = ""
    
    'Path = Application.ActiveWorkbook.Path
    
    'ActiveWorkbook.SaveAs Filename:=Path & "\" & "_" & ActiveWorkbook.Name
    
    
End Sub
